     ]
  },
  {
    "type": "orderedQueriesClassifier",
    "entity": "file",
    "domain": "type",
    "groups": [
     {
         "label": "code",
         "query": "extension in -i {c, h, cc, hh, cpp, hpp, cs,java,dart,sql,cql,kt,groovy,erl,hrl,js,angular,jsx,ts,tsx,jsp, py, pl, cmd, sh, bat, ps1, vbs, feature, story, Feature, steps}" 
